 Update backend\n    const updateDB = () => {\n      const task = tasks.find(({ _ID }) => _ID === id);\n      console.log(task.reminder);\n      axios.put(`http://127.0.0.1:5000/${id}`, { text: task.text, reminder: !task.reminder });\n    };\n\n    setTasks(\n      tasks.map((task) => (task._ID === id ? { ...task, reminder: !task.reminder } : task)),\n      () => updateDB(),\n    );\n  };\n\n  return (\n    <div className=\"container\">\n      <Header onAdd={() => setShowAddTask(!showAddTask)} />\n      {showAddTask && <AddTask onAdd={addTask} />}\n      {tasks.length > 0 ? (<Tasks tasks={tasks} onDelete={deleteTask} onToggle={toggleReminder} />) : ('No Tasks To Show')}\n    </div>\n  );\n}\n\nexport default App;\n","/Users/aaronang/Documents/GitHub/H4I_Impact_Team_Challenge/client/src/components/Header.js",[],"/Users/aaronang/Documents/GitHub/H4I_Impact_Team_Challenge/client/src/components/Tasks.js",[],"/Users/aaronang/Documents/GitHub/H4I_Impact_Team_Challenge/client/src/components/AddTask.js",["44"],"import React, { useState } from 'react';\n\nfunction AddTask({ onAdd }) {\n  const [text, setText] = useState('');\n  // const [day, setDay] = useState('')\n  const [reminder, setReminder] = useState(false);\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n\n    if (!text) {\n      alert('Please add a task');\n      return;\n    }\n\n    // onAdd({ text, day, reminder })\n    onAdd({ text, reminder });\n\n    setText('');\n    // setDay('')\n    setReminder(false);\n  };\n\n  return (\n    <form className=\"add-form\" onSubmit={onSubmit}>\n      <div className=\"form-control\">\n        <label htmlFor=\"task\">\n          Task\n          <input\n            type=\"text\"\n            placeholder=\"Add Task\"\n            value={text}\n            onChange={(e) => setText(e.target.value)}\n          />\n        </label>\n      </div>\n      {/* <div className='form-control'>\n        <label>Day and Time</label>\n        <input\n          type='text'\n          placeholder='Add Day and Time'\n          value={day}\n          onChange={(e) => setDay(e.target.value)}\n        />\n      </div> */}\n      <div className=\"form-control form-control-check\">\n        <label htmlFor=\"reminder\">\n          Set Reminder\n          <input\n            type=\"checkbox\"\n            checked={reminder}\n            onChange={(e) => setReminder(e.currentTarget.checked)}\n          />\n        </label>\n      </div>\n      <input className=\"btn btn-block\" type=\"submit\" value=\"Save Task\" />\n    </form>\n  );\n}\n\nexport default AddTask;\n","/Users/aaronang/Documents/GitHub/H4I_Impact_Team_Challenge/client/src/components/Task.js",[],"/Users/aaronang/Documents/GitHub/H4I_Impact_Team_Challenge/client/src/components/Button.js",[],{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","severity":1,"message":"58","line":42,"column":7,"nodeType":"59","messageId":"60","endLine":42,"endColumn":18},{"ruleId":"61","severity":1,"message":"62","line":12,"column":7,"nodeType":"63","messageId":"60","endLine":12,"endColumn":33},"lines-around-directive",["64"],"no-spaced-func",["65"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"no-console","Unexpected console statement.","MemberExpression","unexpected","no-alert","Unexpected alert.","CallExpression","padding-line-between-statements","func-call-spacing"]